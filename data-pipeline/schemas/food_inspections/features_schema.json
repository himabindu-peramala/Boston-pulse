{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Food Establishment Inspections Features Schema",
    "description": "Schema for Food Establishment Inspections features at grid cell level",
    "type": "object",
    "properties": {
        "grid_cell": {
            "type": "string"
        },
        "neighborhood": {
            "type": "string"
        },
        "inspection_count_180d": {
            "type": "integer"
        },
        "failure_count_180d": {
            "type": "integer"
        },
        "failure_ratio_180d": {
            "type": "number"
        },
        "grid_lat": {
            "type": "number"
        },
        "grid_long": {
            "type": "number"
        }
    },
    "required": [
        "grid_cell",
        "neighborhood"
    ],
    "additionalProperties": true
}