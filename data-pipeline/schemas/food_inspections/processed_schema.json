{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Food Establishment Inspections Processed Data Schema",
    "description": "Schema for processed Food Establishment Inspections data",
    "type": "object",
    "properties": {
        "_id": {
            "type": "integer"
        },
        "businessname": {
            "type": "string"
        },
        "licenseno": {
            "type": [
                "string",
                "integer"
            ]
        },
        "result": {
            "type": "string"
        },
        "resultdttm": {
            "type": [
                "string",
                "datetime"
            ],
            "format": "date-time"
        },
        "address": {
            "type": "string"
        },
        "lat": {
            "type": [
                "number",
                "null"
            ]
        },
        "long": {
            "type": [
                "number",
                "null"
            ]
        },
        "year": {
            "type": "integer"
        },
        "month": {
            "type": "integer"
        },
        "day_of_week": {
            "type": "string"
        }
    },
    "required": [
        "_id",
        "businessname",
        "licenseno",
        "result",
        "resultdttm"
    ],
    "additionalProperties": true
}
