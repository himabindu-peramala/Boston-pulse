{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "CityScore Raw Data Schema",
    "description": "Schema for raw CityScore data from Analyze Boston API",
    "type": "object",
    "properties": {
        "metric_name": {
            "type": "string",
            "description": "Name of the CityScore metric"
        },
        "score_calculated_ts": {
            "type": [
                "string",
                "null"
            ],
            "description": "Timestamp when the score was calculated",
            "format": "date-time",
            "watermark_field": true
        },
        "target": {
            "type": [
                "number",
                "null"
            ],
            "description": "Target value for the metric"
        },
        "day_score": {
            "type": [
                "number",
                "null"
            ],
            "description": "Score for the current day"
        }
    },
    "required": [
        "metric_name",
        "score_calculated_ts"
    ],
    "additionalProperties": true,
    "metadata": {
        "dataset": "cityscore",
        "stage": "raw",
        "source": "Analyze Boston API",
        "resource_id": "dd657c02-3443-4c00-8b29-56a40cfe7ee4",
        "version": "1.0.0"
    }
}