{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "City Owned Property Processed Schema",
    "type": "object",
    "required": [
        "property_id",
        "owner",
        "address",
        "latitude",
        "longitude"
    ],
    "properties": {
        "property_id": {
            "type": "integer"
        },
        "owner": {
            "type": "string"
        },
        "address": {
            "type": "string"
        },
        "neighborhood_id": {
            "type": [
                "string",
                "null"
            ]
        },
        "latitude": {
            "type": "number"
        },
        "longitude": {
            "type": "number"
        },
        "processed_at": {
            "type": "string",
            "format": "date-time"
        }
    }
}
